<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd" >
<!-- struts2的核心配置文件，在应用部署时加载并解析 -->
<struts>
	<package name="realdatapackage" extends="struts-default" namespace="/">
		<action name="open" class="realDataAction" method="open">
		</action>
		<action name="begin" class="realDataAction" method="beginCollect">
		</action>
		<action name="stop" class="realDataAction" method="stopCollect">
		</action>
		<action name="realdata" class="realDataAction" method="realData">
		</action>
	</package>
</struts>
