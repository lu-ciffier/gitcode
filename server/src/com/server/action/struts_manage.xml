<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd" >
<!-- struts2的核心配置文件，在应用部署时加载并解析 -->
<struts>
	<package name="managepackage" extends="struts-default">
		<action name="userinfo" class="manageAction" method="userinfo">
		</action>
		<action name="dropdata" class="manageAction" method="dropdata">
		</action>
		<action name="userquit" class="manageAction" method="userquit">
			<result name="success" type="redirect">
			index.jsp
			</result>
		</action>
		
	</package>
</struts>
