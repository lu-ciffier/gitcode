<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	</type-mapping>
	<table name="points" catalog="ssh" class="com.server.jopo.Point">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="topologys" catalog="ssh"
		class="com.server.jopo.Topology">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="devices" catalog="ssh"
		class="com.server.jopo.Device">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="charts" catalog="ssh" class="com.server.jopo.Chart">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="alerts" catalog="ssh" class="com.server.jopo.Alert">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="alertinfos" catalog="ssh"
		class="com.server.jopo.Alertinfo">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="systems" catalog="gh"
		class="com.server.jopo.SystemConfig">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="weathers" catalog="gh"
		class="com.server.jopo.Weather">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="charts" catalog="gh" class="com.server.jopo.Chart">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>